@model IList<string>
@{
    ViewData["Title"] = "UserRoles";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>UserRoles</h1>
<a asp-action="Index">User List</a>
<table class="table">
    <thead>
        <tr>
            <td>Role Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                @item
            </tr>
        }
    </tbody>
</table>

